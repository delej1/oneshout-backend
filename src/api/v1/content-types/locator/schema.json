{
  "kind": "collectionType",
  "collectionName": "locators",
  "info": {
    "singularName": "locator",
    "pluralName": "locators",
    "displayName": "User Locators",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "locator"
    },
    "lat": {
      "type": "float",
      "required": false
    },
    "lng": {
      "type": "float",
      "required": false
    },
    "locateMe": {
      "type": "boolean",
      "default": false
    },
    "viewedBy": {
      "type": "enumeration",
      "enum": [
        "all",
        "restricted"
      ],
      "default": "restricted"
    },
    "viewers": {
      "type": "string",
      "maxLength": 1500
    }
  }
}
